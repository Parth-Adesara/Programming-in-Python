    "def maxArea(A, Len) :\n",
    "    area = 0\n",
    "    for i in range(Len) :\n",
    "        for j in range(i + 1, Len) :\n",
    "            area = max(area, min(A[j], A[i]) * (j - i))\n",
    "    return area\n",
    " \n",
    "a = [int(n) for n in input(\"Enter an array: \").split()]\n",
    " \n",
    "len1 = len(a)\n",
    "print(maxArea(a, len1))"
   ]
